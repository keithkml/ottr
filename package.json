{
  "name": "@uber/ottr",
  "version": "0.1.0",
  "description": "Run end-to-end tests in a web browser",
  "homepage": "https://code.uberinternal.com/diffusion/WEOTTOC/",
  "bugs": {
    "url": "https://code.uberinternal.com/diffusion/WEOTTOC/"
  },
  "repository": {
    "type": "git",
    "url": "gitolite@code.uber.internal:web/ottr"
  },
  "keywords": [
    "testing",
    "javascript",
    "js"
  ],
  "author": "Keith Lea",
  "license": "MIT",
  "main": "lib/api/index.js",
  "bin": {
    "ottr": "lib/cli/cli.js"
  },
  "scripts": {
    "build": "babel src/main -d lib --source-maps && mkdir -p lib/static && cp -Rv src/main/ui/static/* node_modules/font-awesome/{css,fonts} lib/static",
    "build-test": "npm run build && babel src -d dist-test --source-maps",
    "cover": "cross-env NODE_ENV=test npm run build-test && cross-env NODE_ENV=test nyc node dist-test/test/index.spec.js; nyc report",
    "test": "npm run cover",
    "jenkins": "npm run lint && npm run flow && npm run cover",
    "flow": "flow check",
    "check": "npm run prettier && npm run lint && npm run flow && npm run test",
    "prettier": "prettier --single-quote --bracket-spacing false --parser flow --tab-width 2 --print-width 100 --write \"{src,test}/**/*.js\"",
    "lint": "eslint src",
    "unpm-prepublish": "echo 'no pre-publish script yet'",
    "watch": "nodemon --ext js,html --exec 'npm run build' --watch src",
    "prettier-amend-commit": "npm run prettier && git add -u && git commit --amend --no-edit",
    "pre-arc-diff": "npm run prettier-amend-commit && npm run lint && npm run flow"
  },
  "engines": {
    "node": ">=6.10",
    "npm": ">=4.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "font-awesome": "^4.7.0",
    "get-port": "^3.2.0",
    "http-proxy-middleware": "^0.17.4",
    "http-proxy-response-rewrite": "0.0.1",
    "immer": "^0.6.1",
    "jquery": "^3.2.1",
    "jquery-simulate-ext": "^1.3.0",
    "node-fetch": "^1.7.3",
    "puppeteer": "^1.0.0",
    "react": "^16.2.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.2.0",
    "react-fontawesome": "^1.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-trigger-change": "^1.0.2",
    "redux": "^3.7.2",
    "redux-observable": "^0.17.0",
    "redux-promise": "^0.5.3",
    "rxjs": "^5.5.6",
    "socket.io": "^2.0.4",
    "source-map-support": "^0.4.18",
    "styled-components": "^2.4.0",
    "tape": "^4.6.3",
    "webpack": "^3.10.0",
    "whatwg-fetch": "^2.0.3",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^6.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.1.3",
    "eslint": "^3.19.0",
    "eslint-config-uber-es2015": "^3.1.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.2.1",
    "flow-bin": "^0.56.0",
    "flow-typed": "^2.2.0",
    "nodemon": "^1.14.7",
    "nyc": "^11.4.1",
    "prettier": "^1.7.4",
    "tape-promise": "^2.0.1",
    "tmp": "0.0.33"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 80,
    "reporter": [
      "cobertura",
      "html"
    ]
  }
}
